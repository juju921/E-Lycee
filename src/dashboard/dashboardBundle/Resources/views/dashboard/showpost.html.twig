{% extends "dashboarddashboardBundle::base.html.twig" %}

{% block body %}
    <div id="wrapper">
    <div id="page-wrapper">
    <div class="row">
        <div class="col-lg-12">
            <h1 class="page-header">Articles</h1>
        </div>
        <!-- /.col-lg-12 -->
    </div>
    
<img src="{{ asset("images/news/"~news.urlThumbnail.fileName) }}" alt="" class="img-responsive"/>
<h1>{{ news.titre }}</h1>
<div class="post_commentbox">

    <span><i class="fa fa-calendar"></i> {{ news.date | date('d M Y') }}</span>

    <span><i class="fa fa-pencil"></i>{{ news.auteur }}</span>

</div>
<p> {{ news.content }}</p>

    </div>
    </div>

{% endblock %}